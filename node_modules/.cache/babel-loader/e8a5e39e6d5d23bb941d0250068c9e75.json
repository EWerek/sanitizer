 {\n  name: \"contact\",\n  data() {\n    return {\n      message: \"\",\n      email: \"\",\n      name: \"\",\n      template_params: {\n        reply_to: \"\",\n        from_name: \"\",\n        to_name: \"\",\n        message_html: \"\"\n      }\n    };\n  },\n  methods: {\n    sendEmail() {\n      this.setTemplate();\n      emailjs\n        .send(\n          \"default_service\",\n          \"template_e5G0Xaq6\",\n          this.template_params,\n          \"user_2VhHgP2YBwBzo9HUU1b8B\"\n        )\n        .then(\n          result => {\n            console.log(\"SUCCESS!\", result.status, result.text);\n            alert(\"Thanks, We will resond shortly\");\n            this.clearform();\n          },\n          error => {\n            console.log(\"FAILED...\", error);\n            alert(\"Message failed - please try again\");\n          }\n        );\n    },\n\n    setTemplate() {\n      this.template_params.message_html = this.message;\n      this.template_params.reply_to = this.email;\n      this.template_params.from_name = this.name;\n    },\n    clearform() {\n      this.name = \"\";\n      this.email = \"\";\n      this.name = \"\";\n    }\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}